<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<bean id="aa" class="com.java.di04.AA">
		<property name="name" value="홍길동"></property>
		<property name="list">
			<list>
				<value>10</value>
				<value>20</value>
				<value>30</value>
				<value>40</value>
				<value>50</value>
			</list>
		</property>

		<property name="map">
			<map>
				<entry>
					<key>
						<value>apple</value>
					</key>
					<value>사과</value>
				</entry>
				<entry>
					<key>
						<value>banana</value>
					</key>
					<value>바나나</value>
				</entry>
			</map>
		</property>

		<property name="set">
			<set>
				<value>77</value>
				<value>88</value>
				<value>99</value>
			</set>
		</property>
		
		<property name="properties"> <!-- ?? -->
			<props>
			<prop key="server">192.168.14.34</prop>
			<prop key="connection">5000</prop>
			</props>
		</property>
	</bean>
</beans>
